{
  "name": "vue-simple-progressbar",
  "version": "0.2.1",
  "license": "MIT",
  "author": "arfurs",
  "keywords": [
    "vue plugin",
    "vue",
    "vue.js",
    "progressbar",
    "fake progressbar"
  ],
  "homepage": "https://github.com/arfurs/vue-simple-progressbar-plugin",
  "repository": {
    "type": "git",
    "url": "https://github.com/arfurs/vue-simple-progressbar-plugin.git"
  },
  "private": false,
  "main": "index.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib --name vueSimpleProgressBar ./src/index.js",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "vue": "^2.5.17"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.4",
